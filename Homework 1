namespace Homework1;
class Program
{
    static void Main(string[] args)
    {
        double x =2.5;
        double y =3.3;
        double z;
        z= 4*x*x+3*y;
        Console.Write("X= "+x);
        Console.Write("\tY= "+y);
        Console.Write("The Value of Z is: "+z);
    }
}
